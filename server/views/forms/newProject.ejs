<div class="input-group mb-3">
    <div class="input-group-prepend">
        <span class="input-group-text text-light bg-dark" id="basic-addon1">Title:</span>
    </div>
    <input name="title" type="text" class="form-control" placeholder="" aria-label="Title"
    aria-describedby="basic-addon1" value="" autofocus required>
</div>
<div class="input-group mb-3">
    <div class="input-group-prepend">
        <span class="input-group-text text-light bg-dark" id="basic-addon1">Description:</span>
    </div>
    <input name="desc" type="text" class="form-control" placeholder="" aria-label="Description"
    aria-describedby="basic-addon1" value="">
</div>
<div class="input-group mb-3">
    <div class="input-group-prepend">
        <span class="input-group-text text-light bg-dark" id="basic-addon1">Category:</span>
    </div>
    <select name="category" class="custom-select">
        <option value="Web Development">Web Development</option>
        <option value="E-Commerce">E-Commerce</option>
        <option value="Sound Design">Sound Design</option>
    </select>
</div>
<div class="input-group mb-3">
    <div class="input-group-prepend">
        <span class="input-group-text text-light bg-dark" id="basic-addon1">Image</span>
    </div>
    <input name="img" type="text" class="form-control" placeholder="Add image url" aria-label="image"
    aria-describedby="basic-addon1" value="">
</div>
<div class="input-group mb-3">
    <div class="input-group-prepend">
        <span class="input-group-text text-light bg-dark" id="basic-addon1">Link</span>
    </div>
    <input name="link" type="url" class="form-control" placeholder="Link to project repository" aria-label="url" aria-describedby="basic-addon1"
    value="">
</div>
<div class="input-group mb-3">
    <div class="input-group-prepend">
        <span class="input-group-text text-light bg-dark" id="basic-addon1">Live Demo</span>
    </div>
    <input name="demo" type="url" class="form-control" placeholder="Link to project" aria-label="url" aria-describedby="basic-addon1"
    value="">
</div>
<h5 class="text-light">Features</h5>
<div class="input-group-block project-features">
    <ul id="feature-list" class="list-group"></ul>  
</div>
<div class="input-group">
    <input class="form-control" type="text" name="" id="add-feature-input">
    <div class="input-group-append">
        <button class="btn btn-info" id="add-feature">ADD</button>
    </div>        
</div>
<input id="featuresInput" name="features" type="hidden" name="features" value="">
<h5 class="text-light">Content</h5>
<div class="form-group">
    <textarea name="content" class="form-control" id="exampleFormControlTextarea1" rows="3" placeholder="About the project"></textarea>
</div>
<input type="hidden" name="_csrf" value="<%= csrf %>" />
